    jQuery(document).ready(function($) {
        function moveToAbsoluteTop() {
            // Only run on mobile/tablet
            if ($(window).width() <= 980) {
                setTimeout(function() {
                    // Find the language switcher li element
                    var langSwitcherLi = $('.et_mobile_menu .yn-language-switcher-top, .et_mobile_menu li[class*="language-switcher"], .et_mobile_menu .trp-language-switcher-container').closest('li');
                    
                    // Find the main mobile menu ul
                    var mobileMenuUl = $('#mobile_menu, .et_mobile_menu ul').first();
                    
                    if (langSwitcherLi.length && mobileMenuUl.length) {
                        // Remove from current position
                        langSwitcherLi.detach();
                        
                        // Add special classes
                        langSwitcherLi.addClass('yn-lang-first-item menu-item-first');
                        
                        // Insert as the very first child
                        mobileMenuUl.prepend(langSwitcherLi);
                        
                        console.log('Language switcher moved to absolute top position');
                    } else {
                        // Fallback: look for any language switcher element
                        var anyLangSwitcher = $('.et_mobile_menu').find('*:contains("Swedish"), *:contains("English"), .trp-language-switcher, [class*="language"]').first().closest('li');
                        
                        if (anyLangSwitcher.length && mobileMenuUl.length) {
                            anyLangSwitcher.detach();
                            anyLangSwitcher.addClass('yn-lang-first-item menu-item-first');
                            mobileMenuUl.prepend(anyLangSwitcher);
                            
                            console.log('Language switcher found via fallback and moved to top');
                        }
                    }
                }, 2000); // Increased delay
            }
        }
        
        // Run multiple times to ensure it works
        moveToAbsoluteTop();
        
        // Run when mobile menu is clicked/opened
        $(document).on('click', '.mobile_menu_bar, .et_toggle_left_menu, .mobile_menu_bar_toggle', function() {
            setTimeout(moveToAbsoluteTop, 300);
            setTimeout(moveToAbsoluteTop, 1000);
        });
        
        // Run on window resize
        $(window).resize(function() {
            setTimeout(moveToAbsoluteTop, 500);
        });
        
        // Run periodically for the first few seconds (aggressive approach)
        var attempts = 0;
        var interval = setInterval(function() {
            moveToAbsoluteTop();
            attempts++;
            if (attempts >= 5) {
                clearInterval(interval);
            }
        }, 1000);
    });
    </script>
